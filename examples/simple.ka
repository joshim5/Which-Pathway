%agent: A(b,c)
%agent: B(a,c)
%agent: C(a,b)
%var: 'Kon' 1.0e-4
'A/' -> A(b,c) @ 1.0
'B/' -> B(a,c) @ 1.0
'C/' -> C(a,b) @ 1.0
'A.B' A(b,c), B(a,c) -> A(b!1,c), B(a!1,c) @ 'Kon'
'AB.C' A(b!1,c), B(a!1,c), C(a,b) -> A(b!1,c!3), B(a!1,c!2), C(a!3,b!2) @ 'Kon'
%obs: 'ABC' |A(b!1,c!3), B(a!1,c!2), C(a!3,b!2)|
%def: "displayCompression" "weak"
%mod: [T]>0 do $TRACK 'ABC' [true]
%mod: 'ABC' > 0 do ( $TRACK 'ABC' [false]; $STOP )